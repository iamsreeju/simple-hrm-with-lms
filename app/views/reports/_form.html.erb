<%= simple_form_for(@report) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%#= f.input :report_date %>

    <%= f.input :report_date, as: :date, start_year: Date.today.year - 1, end_year: Date.today.year, order: [:month, :year], label: "Month" %>

    <%#= f.input :report_status %>
  </div>

  <div class="form-actions">
    	<%= f.button :submit, :class => "btn btn-success" %>
  </div>
<% end %>
